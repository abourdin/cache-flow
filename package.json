{
  "name": "cache-flow",
  "version": "1.0.0",
  "description": "A library made for easy implementation of robust NodeJS application caches",
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build --verbose",
    "prepublish": "tsc",
    "test": "ts-mocha \"./test/**/*Test.test\" -p ./tsconfig.json --extension js,jsx,ts,tsx --exit --recursive --reporter mocha-multi-reporters --reporter-options configFile=mocha-config.json --ui bdd --slow 300",
    "test-coverage": "nyc ts-mocha \"./test/**/*Test.test\" -p ./tsconfig.json --extension js,jsx,ts,tsx --exit --recursive --reporter mocha-multi-reporters --reporter-options configFile=mocha-config.json --slow 300",
    "coverage-report": "nyc report"
  },
  "keywords": [],
  "author": "Alexandre Bourdin <contact@abourdin.com>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "date-fns": "^2.16.1",
    "ioredis": "^4.19.2",
    "redis": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/ioredis": "^4.17.7",
    "@types/node": "^14.14.7",
    "@types/redis": "^2.8.28",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "ts-mocha": "^8.0.0"
  }
